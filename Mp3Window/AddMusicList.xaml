<Window x:Class="Mp3Window.AddMusicListxaml"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mp3Window"
        mc:Ignorable="d"
        Title="AddMusicListxaml" Height="300" Width="500" WindowStyle="None">
    <Grid>
        <StackPanel>
            <Label  FontSize="20" Padding="20,20">添加歌单</Label>
            <TextBox Height="50" Margin="50,0" x:Name="TextBox"></TextBox>
            <StackPanel Orientation="Horizontal" Margin="0,50,0,0">
                <Button FontSize="15" Padding="20" Margin="200,0,0,0" Background="#FFAE1111" Foreground="White" Click="Button_Click_1" >确定</Button>
                <Button FontSize="15" Padding="20" Margin="50,0" Foreground="#FFA40606" Background="White" BorderBrush="#FFA40404" Click="Button_Click" >取消</Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
